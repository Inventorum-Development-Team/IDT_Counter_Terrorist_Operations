extended:
  tags:
    BattleUnit:
      Rank: int
      Shouted: int
      FlashBanged: int
      OGfiring: int
      OGreactions: int
    RuleItem:
      Shout: int
      Cuffs: int
      AmmoReSprite: int
      AmmoNo: int
      FoldStock: int
      UBS: int
      Shield: int
